CREATE TABLE USER (ID INTEGER AUTO_INCREMENT NOT NULL, LASTNAME VARCHAR(255), NAME VARCHAR(255), PASSWORD VARCHAR(255), PROJECTID INTEGER, ROLEID INTEGER, USERNAME VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE PROJECT (ID INTEGER AUTO_INCREMENT NOT NULL, IMGURL1 VARCHAR(255), IMGURL2 VARCHAR(255), IMGURL3 VARCHAR(255), PROJECTDETAIL VARCHAR(255), PROJECTNAME VARCHAR(255), SHORTPROJECTDETAIL VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE QUESTION (ID INTEGER AUTO_INCREMENT NOT NULL, TITLE VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE VOTE (ID INTEGER AUTO_INCREMENT NOT NULL, PROJECTID INTEGER, QUESTIONID INTEGER, TIMESTAMPFIELD DATETIME, USERID INTEGER, PRIMARY KEY (ID))
CREATE TABLE LOGIN_LOG (ID INTEGER AUTO_INCREMENT NOT NULL, IP VARCHAR(255), TIMESTAMPFIELD DATETIME, WHO VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE TIME (ID INTEGER AUTO_INCREMENT NOT NULL, DAY INTEGER, HOUR INTEGER, MIN INTEGER, MONTH INTEGER, SEC INTEGER, YEAR INTEGER, PRIMARY KEY (ID))
CREATE TABLE ROLE (ID INTEGER AUTO_INCREMENT NOT NULL, ROLE VARCHAR(255), VOTELIMIT INTEGER, PRIMARY KEY (ID))
